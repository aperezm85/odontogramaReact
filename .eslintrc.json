{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": "off"
  },
  "globals": {
    "alert": true,
    "document": true,
    "expect": true,
    "localStorage": true,
    "navigator": true,
    "test": true,
    "window": true
  }
}
